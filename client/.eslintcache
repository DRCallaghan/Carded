sole.error(e);\n        };\n        window.location.reload();\n    };\n\n    // handle blur function to display error messages in case the user navigates to another app while inputting data\n    const handleBlur = (e) => {\n        e.preventDefault();\n\n        const { target } = e;\n        const inputType = target.name;\n        const inputValue = target.value;\n\n        if ((inputType === 'name') && (!inputValue.length)) {\n            setErrorMessage('Please enter a team name');\n            console.log(setErrorMessage);\n        }\n        if ((inputType === 'address') && (!inputValue.length)) {\n            setErrorMessage('Please include your company');\n            console.log(setErrorMessage);\n        }\n        if ((inputType === 'website') && (!inputValue.length)) {\n            setErrorMessage('Please include a link for your website');\n            console.log(setErrorMessage);\n        }\n    };\n\n    // returning the add team button which displays the modal with form inside\n    const styles = {\n        textStyle: {\n            color: 'red',\n            fontSize: 'large'\n        },\n        buttonStyle: {\n            color: '#14397d'\n        }\n    }\n\n    return (\n        <>\n            <Button variant=\"btn btn-md btn-light m-2\" onClick={handleShow}>\n                Add Team\n            </Button>\n            <Modal show={show} onHide={handleClose}>\n                <Modal.Body>\n                    <Form className='textbox'>\n                        <Form.Group className=\"mb-3\" controlId=\"exampleForm.ControlInput1\">\n                            <Form.Label className='info'>Add your Team name</Form.Label>\n                            <Form.Control className='input'\n                                type=\"text\"\n                                name='name'\n                                placeholder=\"Team Name\"\n                                onBlur={handleBlur}\n                                onChange={handleChange}\n                                value={formState.name}\n                                autoFocus\n                            />\n                        </Form.Group>\n                        <Form.Group className=\"mb-3\" controlId=\"exampleForm.ControlInput1\">\n                            <Form.Label className='info'>Add your Company Address</Form.Label>\n                            <Form.Control className='input'\n                                type=\"text\"\n                                name='address'\n                                placeholder=\"123 Main Street\"\n                                onBlur={handleBlur}\n                                onChange={handleChange}\n                                value={formState.address}\n                                autoFocus\n                            />\n                        </Form.Group>\n                        <Form.Group className=\"mb-3\" controlId=\"exampleForm.ControlInput1\">\n                            <Form.Label className='info'>Add a Link to your Website</Form.Label>\n                            <Form.Control className='input'\n                                type=\"text\"\n                                name='website'\n                                placeholder=\"example.com\"\n                                onBlur={handleBlur}\n                                onChange={handleChange}\n                                value={formState.website}\n                                autoFocus\n                            />\n\n                        </Form.Group>\n                        <div style={styles.textStyle}>\n                            {errorMessage && (<div className=\"error-text\">{errorMessage}</div>)}\n                        </div>\n                        <Modal.Footer className='buttons'>\n                            <Button className='button-close btn btn-danger m-2' variant=\"secondary\" onClick={handleClose}>\n                                Close\n                            </Button>\n                            <Button style={styles.buttonStyle} className='button-save btn btn-light m-2 ' variant=\"primary\" onClick={handleSubmit}>\n                              Save Changes\n                            </Button>\n                        </Modal.Footer>\n                    </Form>\n                </Modal.Body>\n            </Modal>\n        </>\n    );\n}\n\nexport default Team;","/Users/petewang/Documents/Carded/client/src/utils/auth.js",[],"/Users/petewang/Documents/Carded/client/src/components/Bootstrap/card.js",["158"],"import React from \"react\";\nimport Card from \"react-bootstrap/Card\";\nimport { FaLinkedin } from \"react-icons/fa\";\n\n//          BELOW TO RENDER DATA ON THE CARD\nimport { useParams } from 'react-router-dom';\nimport { QUERY_SINGLE_PROFILE, QUERY_ME } from '../../utils/queries';\nimport { useQuery } from '@apollo/client';\n\n\n\n\nfunction CardExample() {\n\n\n\n  //          BELOW TO RENDER DATA ON THE CARD\n//     BASIC LOGIC IS SET UP - CURRENLTY WILL RENDER USER NAME AND ANY USER PROPERTY ATTACTHED IF IT IS PRESENT - ID, ADDRESS, TEAM ...  THIS IS NOT COMING OFF OF THE ADD A TEAM MEMBER MODAL HOWEVER.  IT CAN BE REFACTORED TO TAKE THAT INFO ONCE IT IS ACCESSIBLE, OR THAT INFO CAN BE REFACTGORED TO COME IN FROM ANOTHER SIGN UP AREA TO BE USED HERE...\n\n  const { profileId } = useParams();\n  const { loading, data } = useQuery(\n    profileId ? QUERY_SINGLE_PROFILE : QUERY_ME,\n    {\n      variables: { profileId: profileId },\n    }\n  );\n  console.log(data.me.team)\n  return (\n    <div className=\"cardManager\">\n    <Card style={{ width: \"23rem\" }}>\n      <Card.Body>\n        <Card.Title>\n          <h3>\n        Name: {data.me.name}\n          </h3>\n        </Card.Title>\n        <Card.Subtitle className=\"text-muted\">\n          <h4>\n          Position:\n          </h4>\n          </Card.Subtitle>\n        <Card.Subtitle className=\"mb-2 text-muted\">\n          <h4>\n          Team:\n          </h4>\n          </Card.Subtitle>\n        <Card.Text>\n          Some quick example text to build on the card title and make up the\n          bulk of the card's content.\n        </Card.Text>\n        {/* <Card.Link href=\"#\">LinkedIn</Card.Link> */}\n        <a style = {{fontSize: \"2rem\"}} href=\" https://www.linkedin.com/in/pete-wang-SWE\" target=\"blank\">\n          <FaLinkedin />\n        </a>\n      </Card.Body> \n    </Card>\n    </div>\n  );\n}\n\n\n\n\n// function CardExample() {\n//   return (\n//     <Card style={{ width: \"18rem\" }}>\n//       <Card.Body>\n//         <Card.Title>Name</Card.Title>\n//         <Card.Subtitle className=\"mb-2 text-muted\">Position</Card.Subtitle>\n//         <Card.Text>\n//           Some quick example text to build on the card title and make up the\n//           bulk of the card's content.\n//         </Card.Text>\n//         {/* <Card.Link href=\"#\">LinkedIn</Card.Link> */}\n//         <a style = {{fontSize: \"2rem\"}} href=\" https://www.linkedin.com/in/pete-wang-SWE\" target=\"blank\">\n//           <FaLinkedin />\n//         </a>\n//       </Card.Body> \n//     </Card>\n//   );\n// }\n\nexport default CardExample;\n","/Users/petewang/Documents/Carded/client/src/components/Bootstrap/backButton.js",[],"/Users/petewang/Documents/Carded/client/src/components/Bootstrap/memberCard.js",[],"/Users/petewang/Documents/Carded/client/src/components/PhoneList/index.js",["159"],"import React from 'react';\nimport { useMutation } from '@apollo/client';\n\nimport { REMOVE_PHONE } from '../../utils/mutations';\nimport { QUERY_ME } from '../../utils/queries';\n\nconst PhoneList = ({ phoneNumber, isLoggedInUser = false }) => {\n  const [removePhone, { error }] = useMutation(REMOVE_PHONE, {\n    update(cache, { data: { removePhone } }) {\n      try {\n        cache.writeQuery({\n          query: QUERY_ME,\n          data: { me: removePhone },\n        });\n      } catch (e) {\n        console.error(e);\n      }\n    },\n  });\n\n  const handleRemovePhone = async (phone) => {\n    try {\n      const { data } = await removePhone({\n        variables: { phone },\n      });\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  if (!phoneNumber.length) {\n    return <h3>No Phone Number Yet</h3>;\n  }\n\n  return (\n    <div>\n      <div className=\"flex-row justify-space-between my-4\">\n        {phoneNumber &&\n          phoneNumber.map((phone) => (\n            <div key={phone} className=\"col-12 col-xl-6\">\n              <div className=\"card mb-3\">\n                <h4 className=\"card-header bg-dark text-light p-2 m-0 display-flex align-center\">\n                  <span>{phone}</span>\n                  {isLoggedInUser && (\n                    <button\n                      className=\"btn btn-sm btn-danger ml-auto\"\n                      onClick={() => handleRemovePhone(phone)}\n                    >\n                      X\n                    </button>\n                  )}\n                </h4>\n              </div>\n            </div>\n          ))}\n      </div>\n      {error && (\n        <div className=\"my-3 p-3 bg-danger text-white\">{error.message}</div>\n      )}\n    </div>\n  );\n};\n\nexport default PhoneList;\n","/Users/petewang/Documents/Carded/client/src/components/PhoneForm/index.js",["160"],"import React, { useState } from 'react';\nimport { Link } from 'react-router-dom';\nimport { useMutation } from '@apollo/client';\n\nimport { ADD_PHONE } from '../../utils/mutations';\n\nimport Auth from '../../utils/auth';\n\nconst PhoneForm = ({ profileId }) => {\n  const [phone, setPhone] = useState('');\n\n  const [addPhone, { error }] = useMutation(ADD_PHONE);\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n\n    try {\n      const data = await addPhone({\n        variables: { profileId, phone },\n      });\n\n      setPhone('');\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  return (\n    <div>\n      <h4>Endorse some more phones below.</h4>\n\n      {Auth.loggedIn() ? (\n        <form\n          className=\"flex-row justify-center justify-space-between-md align-center\"\n          onSubmit={handleFormSubmit}\n        >\n          <div className=\"col-12 col-lg-9\">\n            <input\n              placeholder=\"Endorse some phones...\"\n              value={phone}\n              className=\"form-input w-100\"\n              onChange={(event) => setPhone(event.target.value)}\n            />\n          </div>\n\n          <div className=\"col-12 col-lg-3\">\n            <button className=\"btn btn-info btn-block py-3\" type=\"submit\">\n              Endorse phone\n            </button>\n          </div>\n          {error && (\n            <div className=\"col-12 my-3 bg-danger text-white p-3\">\n              {error.message}\n            </div>\n          )}\n        </form>\n      ) : (\n        <p>\n          You need to be logged in to endorse phones. Please{' '}\n          <Link to=\"/login\">login</Link> or <Link to=\"/signup\">signup.</Link>\n        </p>\n      )}\n    </div>\n  );\n};\n\nexport default PhoneForm;\n","/Users/petewang/Documents/Carded/client/src/components/ProfileList/index.js",[],"/Users/petewang/Documents/Carded/client/src/components/Header/index.js",[],"/Users/petewang/Documents/Carded/client/src/components/Bootstrap/searchMember.js",[],{"ruleId":"161","severity":1,"message":"162","line":4,"column":8,"nodeType":"163","messageId":"164","endLine":4,"endColumn":17},{"ruleId":"161","severity":1,"message":"165","line":5,"column":8,"nodeType":"163","messageId":"164","endLine":5,"endColumn":17},{"ruleId":"161","severity":1,"message":"166","line":11,"column":8,"nodeType":"163","messageId":"164","endLine":11,"endColumn":19},{"ruleId":"161","severity":1,"message":"167","line":31,"column":20,"nodeType":"163","messageId":"164","endLine":31,"endColumn":24},{"ruleId":"161","severity":1,"message":"168","line":19,"column":10,"nodeType":"163","messageId":"164",